{% schema %}
  {
    "name": "Hair care routine",
    "class": "section-customised-product",
    "tag": "section",
    "settings": [
      {
        "type": "color",
        "id": "section_bg",
        "label": "Background color",
        "default": "#FFFFFF"
      },
