{%- style -%}
  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
  }
  @media screen and (min-width: 750px) {
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }
{%- endstyle -%}

<div class="relative section-{{ section.id }}-padding" style="background-color:{{ section.settings.section_bg }};">
  <div class="container mx-auto px-4">
    <div id="tab-head" class="-mx-4 mb-8 md:mx-auto">
      <div class="flex items-center gap-1 border-b border-solid border-black border-opacity-10">
        {% if section.settings.title != blank %}
          <div class="tab-btn before:content[''] relative m-auto w-full py-4 text-center before:absolute before:bottom-0 before:left-0 before:h-full before:w-full before:border-b before:border-solid before:border-black" data-tab="hair-care">
            <a href="#" class="block font-AnekLatin-Bold text-[14px]/[26px] uppercase text-black">
              {{- section.settings.title -}}
            </a>
          </div>
        {% endif %}
        {% if section.settings.title_2 != blank %}
          <div class="tab-btn relative m-auto w-full py-4 text-center" data-tab="skin-care">
            <a href="#" class="block font-AnekLatin-Bold text-[14px]/[26px] uppercase text-black text-opacity-60">
              {{- section.settings.title_2 -}}
            </a>
          </div>
        {% endif %}
      </div>
    </div>
    <div class="before:content[''] relative mb-6 mt-11 flex flex-col items-center pb-6 before:absolute before:bottom-0 before:left-0 before:right-0 before:mx-auto before:h-full before:w-[100px] before:border-b before:border-solid before:border-black before:border-opacity-10">
      <h2 class="font-AnekLatin-Bold text-[18px]/[26px] text-black">Your 3-Step Hair Routine</h2>
      <p class="text-[12px]/[26px] text-black">Purchased on Jul 8, 2023</p>
    </div>
    <div class="mb-11 mt-8 flex flex-col items-center gap-3 text-center">
      <h2 class="font-AnekLatin-Bold text-[18px]/[26px] text-black">Get your customised Hair Routine for 10% off</h2>
      <p class="text-[14px]/[19px] text-black">
        Lorem ipsum dolor sit amet consectetur. Aliquet nec morbi urna purus ut amet felis lobortis.
      </p>
      <div class="mt-3 text-center w-full">
        <a
          href="{{ section.settings.button_link }}"
          class="btn w-full"
          role="button">
          <span class="mb-[-2px] mt-[2px]">Take Hair Quiz</span>
        </a>
      </div>
      <div class="mt-3 flex flex-col gap-3">
        <p class="before:content[''] after:content[''] relative w-full text-center font-AnekLatin-SemiBold text-[14px]/[26px] uppercase text-silver-chalice before:absolute before:left-0 before:top-1/2 before:h-[1px] before:w-1/2 before:-translate-y-1/2 before:transform before:border-t before:border-solid before:border-silver-chalice after:absolute after:right-0 after:top-1/2 after:h-[1px] after:w-1/2 after:-translate-y-1/2 after:transform after:border-t after:border-solid after:border-silver-chalice">
          <span class="relative z-10 bg-white px-3">or</span>
        </p>
        <a href="#" class="font-AnekLatin-Bold text-[16px]/[26px] text-black">Take Hair quiz again</a>
      </div>
    </div>
    <div class="flex flex-col">
      <h2 class="relative flex items-baseline gap-4 pr-6 font-AnekLatin-Bold text-[28px]/[26px] text-mine-shaft md:font-AnekLatin-SemiBold md:text-[42px]/[58px]">
        Month 1 of 3
        <span class="absolute right-0 top-6 flex items-center">
