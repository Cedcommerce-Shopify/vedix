      <span class="absolute right-0 top-6 flex items-center">
        <svg
          xmlns="http://www.w3.org/2000/svg"
          width="18.826"
          height="11.029"
          viewBox="0 0 18.826 11.029">
          <path
            id="Vector"
            d="M1,17,9.615,9,1,1"
            transform="translate(0.413 10.615) rotate(-90)"
            fill="none"
            stroke="#000"
            stroke-linecap="round"
            stroke-linejoin="round"
            stroke-width="2" />
        </svg>
      </span>
