                <li class="flex items-center gap-x-[6px] text-[12px]/[20px] text-mine-shaft">
                  <svg
                    xmlns="http://www.w3.org/2000/svg"
                    width="13.43"
                    height="12.302"
                    viewBox="0 0 13.43 12.302">
                    <g
                      id="Group_5668"
                      data-name="Group 5668"
                      transform="translate(-1.852 -2.141)">
                      <path
                        id="Path_7848"
                        data-name="Path 7848"
                        d="M8.047,2.141a6.153,6.153,0,1,0,5.645,8.552.519.519,0,1,0-.956-.4,5.11,5.11,0,1,1,.257-3.228l-.463-.406a.516.516,0,1,0-.679.778l1.536,1.346a.517.517,0,0,0,.786-.126L15.21,6.9a.517.517,0,0,0-.889-.527l-.3.518A6.171,6.171,0,0,0,8.633,2.172q-.295-.03-.586-.031ZM7.58,5.1a.516.516,0,0,0-.519.515V9.446a.517.517,0,0,0,.324.481l2.223.9a.517.517,0,0,0,.384-.96L8.095,9.1V5.619A.516.516,0,0,0,7.58,5.1Z"
